<div class="container">
<h1>Create new review</h1>
<p>Find me in app/views/reviews/new.html.erb</p>
</div>

<div class="container my-5">
  <h1>Review for <%= @cocktail.name %></h1>
  <%= simple_form_for([@cocktail, @review]) do |f| %>
    <%= f.input :content %>
    <%= f.submit "Save", class: "btn btn-ghost" %>
  <% end %>
</div>
